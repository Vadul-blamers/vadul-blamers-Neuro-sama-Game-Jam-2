; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="vadul-blamers-neuro-jam-2"
run/main_scene="res://menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

neuro_sdk="*res://addons/neuro-sdk/neuro_sdk.tscn"

[display]

window/size/viewport_width=1288
window/size/viewport_height=728
window/size/resizable=false
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="vadul-blamers-neuro-jam-2"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/neuro-sdk/plugin.cfg")

[github_to_itch]

config/itch_username="Vadul Blamers "
config/itch_project_name="neuro-jam-2024"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
